<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>OS</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <max_time_warning>
                        <TCEforms>
                            <label>Milliseconds for Warning</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>int</eval>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </max_time_warning>
                    >
                    <max_time_error>
                        <TCEforms>
                            <label>Milliseconds for Error</label>
                            <config>
                                <type>input</type>
                                <size>10</size>
                                <eval>int</eval>
                                <default>0</default>
                            </config>
                        </TCEforms>
                    </max_time_error>
                    >
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
